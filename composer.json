{
    "name": "florian-palabost/laravel-template",
    "type": "project",
    "description": "Laravel template",
    "keywords": [
        "template",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "spatie/laravel-health": "^1.23"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "driftingly/rector-laravel": "^0.27.0",
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "^2.6",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "mohammedmanssour/form-request-tester": "^1.2",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.12",
        "phpunit/phpunit": "^10.0",
        "rector/rector": "^0.17.10",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "clean": [
            "./vendor/bin/rector process",
            "./vendor/bin/pint --dirty",
            "./vendor/bin/phpstan -n"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate -n",
            "@php artisan ide-helper:models -n",
            "@php artisan ide-helper:eloquent -n",
            "@php artisan ide-helper:meta -n"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "@ide-helper"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@ide-helper"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
